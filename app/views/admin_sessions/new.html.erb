<h1>Login</h1>

<% form_for @admin_session, :url => auth_login_path do |f| %>
  <%= f.error_messages %>
  <div>
    <%= f.label :login %>
    <%= f.text_field :login %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.check_box :remember_me %><%= f.label :remember_me %>
  </div>
  <div>
    <%= f.submit "Login" %>
  </div>
<% end %>

